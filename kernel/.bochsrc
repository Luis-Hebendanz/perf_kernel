# configuration file generated by Bochs
plugin_ctrl: iodebug=true, ne2k=false, busmouse=false, e1000=false, sb16=false, speaker=true, usb_uhci=false, es1370=false, pcipnic=false, usb_ehci=false, gameport=true, serial=true, usb_xhci=false, biosdev=true, usb_ohci=false, parallel=true, extfpuirq=true, unmapped=true, voodoo=false
config_interface: textconfig
display_library: term
#display_library: x, options="gui_debug"
memory: host=512, guest=2048
boot: cdrom
ata0: enabled=true, ioaddr1=0x1f0, ioaddr2=0x3f0, irq=14
ata0-master: type=cdrom, path="target/x86_64-unknown-none/debug/perf_kernel.iso", status=inserted, model="Generic 1234", biosdetect=auto
#debug_symbols: file="target/x86_64-unknown-none/debug/perf_kernel.sym"
debugger_log: "target/bochs_debugger.log"
log: "target/bochs.log"
pci: enabled=1, chipset=i440fx, slot1=none, slot2=none, slot3=none, slot4=none, slot5=none
vga: extension=vbe, update_freq=5, realtime=1, ddc=builtin
cpu: count=1:1:1, ips=4000000, quantum=16, model=ryzen, reset_on_triple_fault=1, cpuid_limit_winnt=0, ignore_bad_msrs=1
print_timestamps: enabled=0
logprefix: %t%e%d
magic_break: enabled=1
port_e9_hack: enabled=1
private_colormap: enabled=0
clock: sync=realtime, time0=local, rtc_sync=0
debug: action=ignore
info: action=report
error: action=report
panic: action=ask
keyboard: type=mf, serial_delay=250, paste_delay=100000, user_shortcut=none
mouse: type=ps2, enabled=false, toggle=ctrl+mbutton
sound: waveoutdrv=dummy, waveout=none, waveindrv=dummy, wavein=none, midioutdrv=dummy, midiout=none
speaker: enabled=false
com1: enabled=true, mode=file, dev="/dev/stdout"
parport1: enabled=true, file="target/parallel01.log"
